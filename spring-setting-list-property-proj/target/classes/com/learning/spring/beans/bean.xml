<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="colorsBean" class="com.learning.spring.test.Colors">
		<property name="colors">
			<list>
				<value>Red</value>
				<value>Blue</value>
				<value>Green</value>
				<value>Yellow</value>
				<value>White</value>
				<value>Black</value>
				<value>purple</value>
				<value>Green</value>
			</list>
		</property>
	</bean>
	
	<bean id="colorsBean1" class="com.learning.spring.test.Colors">
		<property name="colors">
			<set>
				<value>Red</value>
				<value>Blue</value>
				<value>Green</value>
				<value>Yellow</value>
				<value>White</value>
				<value>Black</value>
				<value>purple</value>
				<value>Green</value>
			</set>
		</property>
	</bean>
</beans>
