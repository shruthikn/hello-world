<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="jungle" class="com.learning.spring.test.Jungle">
		<property name="largest">
			<bean class="com.learning.spring.test.Animal">
				<property name="name" value="Bheema"></property>
				<property name="type" value="elephant"></property>
			</bean>
		</property>
		<property name="animals">
			<list>
				<bean class="com.learning.spring.test.Animal">
					<property name="name" value="Igor"></property>
					<property name="type" value="lion"></property>
				</bean>
				<bean class="com.learning.spring.test.Animal">
					<property name="name" value="Bheema"></property>
					<property name="type" value="elephant"></property>
				</bean>
				<bean class="com.learning.spring.test.Animal">
					<property name="name" value="john"></property>
					<property name="type" value="bear"></property>
				</bean>
			</list>
		</property>
	</bean>

</beans>
