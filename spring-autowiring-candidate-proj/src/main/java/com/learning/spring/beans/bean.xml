<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName">


	<bean id="logger" class="com.learning.spring.test.Logger">
	</bean>

	<bean id="consoleWriter" class="com.learning.spring.test.ConsoleWriter">
	</bean>

	<bean id="fileWriter" class="com.learning.spring.test.FileWriter">
	</bean>
	
	<bean id="myWriter" class="com.learning.spring.test.FileWriter" autowire-candidate="false" >
	</bean>
	
</beans>
